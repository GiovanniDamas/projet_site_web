<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
    <navigation-rule>

		<!-- page de départ -->
		<from-view-id>/gestionCategories.xhtml</from-view-id>

		<!-- 1.2 cas de navigation : de gestionCategories.xhtml vers gestionCategories.xhtml -->
		<navigation-case>
			<!-- 1.2.1 clé d'outcom -->
			<from-outcome>gestionCat</from-outcome>
			<!-- 1.2.2 destination -->
			<to-view-id>/gestionCategories.xhtml</to-view-id>
			<!-- redirection -->
			<redirect />
		</navigation-case>

		<!-- 1.2 cas de navigation : de gestionCategories.xhtml vers ajouterCategories.xhtml -->
		<navigation-case>
			<!-- 1.2.1 clé d'outcom -->
			<from-outcome>addCat</from-outcome>
			<!-- 1.2.2 destination -->
			<to-view-id>/ajouterCategories.xhtml</to-view-id>
			<!-- redirection -->
			<redirect />
		</navigation-case>

		<!-- 1.2 cas de navigation : de accueil.xhtml vers accueil.xhtml -->
		<navigation-case>
			<!-- 1.2.1 clé d'outcom -->
			<from-outcome>modifCat</from-outcome>
			<!-- 1.2.2 destination -->
			<to-view-id>/modifierCategories.xhtml</to-view-id>
			<!-- redirection -->
			<redirect />
		</navigation-case>
		<!-- 1.2 cas de navigation : de accueil.xhtml vers accueil.xhtml -->
		<navigation-case>
			<!-- 1.2.1 clé d'outcom -->
			<from-outcome>gestionUser</from-outcome>
			<!-- 1.2.2 destination -->
			<to-view-id>/gestionUser.xhtml</to-view-id>
			<!-- redirection -->
			<redirect />
		</navigation-case>

	</navigation-rule>

	<!-- 1 règle de naviation pour la page 'ajouterCategories.xhtml' -->
	<!-- ================================================= -->
	<navigation-rule>

		<!-- page de départ -->
		<from-view-id>/ajouterCategories.xhtml</from-view-id>

		<!-- 1.2 cas de navigation : de accueil.xhtml vers accueil.xhtml -->
