			<from-outcome>modifProduit</from-outcome>
			<to-view-id>/editerProduit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>

		<from-view-id>/Magasin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>magasinfiltre</from-outcome>
			<to-view-id>/MagasinFiltre.xhtml</to-view-id>
